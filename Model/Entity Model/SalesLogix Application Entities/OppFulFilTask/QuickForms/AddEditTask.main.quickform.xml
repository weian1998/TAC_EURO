        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>    // TAC Custom Code Here
    try
    {

        //TAC Load Action for Stages 
        Sage.Entity.Interfaces.IOppFulFilTask _entity = BindingSource.Current as Sage.Entity.Interfaces.IOppFulFilTask;
        object stageId;
        if (DialogService.DialogParameters.TryGetValue("StageId", out stageId))
            _entity.OppFulFilStage = Sage.Platform.EntityFactory.GetById&lt;Sage.Entity.Interfaces.IOppFulFilStage&gt;(stageId.ToString());
        Sage.Entity.Interfaces.IOppFulFilStage parent = _entity.OppFulFilStage;
        // TODO: Complete business rule implementation
        Sage.Entity.Interfaces.IOpportunity Opportunity = parent.Opportunity;
        _entity.Opportunity = Opportunity;
       //entity.Opportunityid = Opportunity.Id.ToString();
       //entity.StageID = stageId.ToString();


        int MaxTaskSequence = 0;
        if (_entity.TaskSequence == null)
        {
            //New Endity so Set the Defaults
            _entity.Priority = "Normal";
            pklPriority.PickListValue = "Normal";
            _entity.Status = "Open";
            pklStatus.PickListValue = "Open";
            //_entity.PercentageComplete = 0;
            //PercentageComplete.Text = "0";
			_entity.DaysFromDeliveryDate = 0;
			if (_entity.Opportunity.DeliveryDate != null)
            {
                _entity.DueDate = _entity.Opportunity.DeliveryDate;
            }
            else
            {
                // Rare Case of null deliverydate
                _entity.DueDate = System.DateTime.Now.ToUniversalTime();
                
            } 
            foreach (Sage.Entity.Interfaces.IOppFulFilTask tmpTask in parent.OppFulFilTasks)
            {
                if (tmpTask.TaskSequence != null)
                {
                    if (tmpTask.TaskSequence &gt; MaxTaskSequence)
                    {
                        MaxTaskSequence = (int)tmpTask.TaskSequence;
                    }
                }
            }
			_entity.ASSIGNEDTOID = Opportunity.AccountManager.Id.ToString (); 
			usrAssignedto.LookupResultValue = Opportunity.AccountManager.Id.ToString (); 
			
            MaxTaskSequence += 10; // Add 10 to the Max
            _entity.TaskSequence = MaxTaskSequence;
            TaskSequence.Text = MaxTaskSequence.ToString();  // Set the Interface

        }

    }
    catch (Exception)
    {


    }</CSharpCodeSnippet>
        </CSharpSnippetActionItem>
      </Action>
