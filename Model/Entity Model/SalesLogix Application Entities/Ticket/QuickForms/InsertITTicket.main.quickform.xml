  <LoadActions>
    <FormActionDefinition>
      <ResourceKey>a0b062c2-dda2-45cf-91dd-2f461e62797c</ResourceKey>
      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet> // TAC Code Here
	lueAccount.ReadOnly = true;
    lueAccount.Enabled = true;
   
        if (this.Request["type"] == "IT")
        {
            Sage.Entity.Interfaces.ITicket _entity = BindingSource.Current as Sage.Entity.Interfaces.ITicket;

            // Set the Orginator Team.
            string Originatorid = _entity.EuroGetTicketOriginatorTeam();
            Sage.Entity.Interfaces.IOwner Originator = Sage.Platform.EntityFactory.GetById&lt;Sage.Entity.Interfaces.IOwner&gt;(Originatorid);
            _entity.Owner = Originator;

            //Set the Assignment
            string AssignToid = _entity.EuroGetTicketAssignedtoid();
            Sage.Entity.Interfaces.IOwner AssignedTo = Sage.Platform.EntityFactory.GetById&lt;Sage.Entity.Interfaces.IOwner&gt;(AssignToid);
            _entity.AssignedTo = AssignedTo;
            ownOwner.LookupResultValue = AssignedTo;
            ownOwner.ReadOnly = true;

            // Set the Assignment to ReadOnly

            //Set IT Company
            string ITAccountid = _entity.EuroGetTicketITCompanyId();
            Sage.Entity.Interfaces.IAccount ItAccount = Sage.Platform.EntityFactory.GetById&lt;Sage.Entity.Interfaces.IAccount&gt;(ITAccountid);
            _entity.Account = ItAccount;
            lueAccount.LookupResultValue = ItAccount;
            // Set IT Contact
            string ITContactid = _entity.EuroGetTicketITContactId();
            Sage.Entity.Interfaces.IContact ITContact = Sage.Platform.EntityFactory.GetById&lt;Sage.Entity.Interfaces.IContact&gt;(ITContactid);
            _entity.Contact = ITContact;
            lueContact.LookupResultValue = ITContact;

            // Set IT Level
            string ITLevel = _entity.EuroGetTicketDefaultTicketLevel();
            _entity.ITTicketLevel = ITLevel;
            pklITTicketLevel.PickListValue = ITLevel;
            pklITTicketLevel.Visible = true;

            // Has Access to Change IT Level
            
            // AREA
            _entity.Area = "IT";
            
                if (_entity.EuroCanChangeITTicketLevel())
            {
                 // Has access to change
                lueContact.ReadOnly = false;
                lueContact.Enabled = true ;
                pklITTicketLevel.ReadOnly = false;
                pklITTicketLevel.Enabled = true;
            }
            else
            { // No Access Read Only
                lueContact.ReadOnly = true;
                lueContact.Enabled = false ;
                pklITTicketLevel.ReadOnly = true;
                pklITTicketLevel.Enabled = false;
            } 
            
            
        }</CSharpCodeSnippet>
        </CSharpSnippetActionItem>
      </Action>
      <IsDialogCloseAction>False</IsDialogCloseAction>
      <RunOnEveryRePaint>False</RunOnEveryRePaint>
      <DialogActionResult>NoResult</DialogActionResult>
      <PerformsUpdate>False</PerformsUpdate>
      <AppliedSecurity />
      <RefreshData>False</RefreshData>
    </FormActionDefinition>
  </LoadActions>
